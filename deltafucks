-- Delta Executor Warning Only (5 Minutes)

local executor = (identifyexecutor and identifyexecutor() or "Unknown")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

if executor == "Delta" then
    local gui = Instance.new("ScreenGui")
    gui.Name = "DeltaWarningGUI"
    gui.Parent = PlayerGui
    gui.IgnoreGuiInset = true

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0.5,0,0.25,0)
    frame.Position = UDim2.new(0.5,0,0.5,0)
    frame.AnchorPoint = Vector2.new(0.5,0.5)
    frame.BackgroundColor3 = Color3.fromRGB(255,80,80)
    frame.BorderSizePixel = 0
    frame.Parent = gui
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0,16)

    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, -20, 1, -20)
    label.Position = UDim2.new(0,10,0,10)
    label.BackgroundTransparency = 1
    label.Text = "⚠️ DON'T USE DELTA!\nPlease use KRNL or another supported executor."
    label.TextColor3 = Color3.fromRGB(255,255,255)
    label.TextScaled = true
    label.TextWrapped = true
    label.Font = Enum.Font.GothamBlack
    label.TextXAlignment = Enum.TextXAlignment.Center
    label.TextYAlignment = Enum.TextYAlignment.Center
    label.Parent = frame

    -- Auto close after 5 minutes (300 seconds)
    delay(300, function()
        if gui then gui:Destroy() end
    end)
end
